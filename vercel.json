{
    "version": 2,
    "builds": [
      {
        "src": "frontend/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "images/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/images/(.*)",
        "dest": "/images/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/index.html"
      }
    ]
  }
  