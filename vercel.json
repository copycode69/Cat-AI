{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/**",
        "use": "@vercel/static"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "Frontend/$1"
      }
    ]
  }